import java.util.Scanner;

public class ProcessaTabuada {

	public static void main(String[] args) {

		int num1 = 1;
		int num2 = 3;

//		System.out.println("Insira o valor da tabuada");
//		Scanner tecla = new Scanner(System.in);
		int resultado = 0;
//		int numero = tecla.nextInt();
		for(int i = 1; i <= 10; i++) {
			System.out.println("Tabuada de " + i);
			for(int j = 1; j < 10; j++) {
				
				resultado = i * j;
				System.out.println(j + "*" + i +"="+ resultado);
			}
		}
	}

}
